<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent">
	<AbsoluteLayout android:id="@+id/absoluteLayout1"
		android:layout_height="fill_parent" android:layout_width="fill_parent">
		<LinearLayout android:id="@+id/linearLayout2"
			android:orientation="vertical"
			android:layout_width="fill_parent" android:layout_height="fill_parent">
			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:id="@+id/linearLayout1"
				android:gravity="center" android:layout_weight="1">
			</LinearLayout>
		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout3"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:paddingTop="10px" android:gravity="right"
			android:paddingRight="20px">
			<Button android:layout_width="76dip"
				android:layout_height="25dip" android:background="@drawable/close_button_xml"
				android:id="@+id/button1"></Button>
		</LinearLayout>
	</AbsoluteLayout>
</LinearLayout>
